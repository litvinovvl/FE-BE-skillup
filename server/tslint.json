{
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "curly": false,
    "variable-name": false,
    "jsx-boolean-value": false,
    "no-var-requires": false
  }
}
